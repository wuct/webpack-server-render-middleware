{
  "name": "webpack-server-render-middleware",
  "version": "0.1.3",
  "description": "Webpack's dev middleware for server-side rendering",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run lint && ava",
    "prebuild": "rm -rf lib",
    "build": "babel --out-dir lib index.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuct/webpack-server-render-middleware.git"
  },
  "keywords": [
    "webpack",
    "server",
    "render",
    "react",
    "universal",
    "isomorphic"
  ],
  "author": "wuct",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wuct/webpack-server-render-middleware/issues"
  },
  "homepage": "https://github.com/wuct/webpack-server-render-middleware#readme",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "./test/*.test.js"
    ],
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.0.0",
    "express": "^4.14.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "eval-as-module": "^0.1.2",
    "memory-fs": "^0.4.0"
  },
  "peerDependencies": {
    "webpack": "^1.0.0 || ^2.0.0 || ^3.0.0"
  }
}
